                <path fill="#F4F3EC" id="Icon_ionic-md-arrow-back" data-name="Icon ionic-md-arrow-back" d="M17.688,11.1H8.795l4.1-4.1L11.833,5.977,5.977,11.833l5.856,5.856,1.025-1.025-4.062-4.1h8.894Z" transform="translate(25.015 8.282) rotate(135)"/>
              </svg>
      .press-block__group2
        each val in [1,2,3,4,5]
          .press-card2
            .press-card2__img
              img(src="./assets/images/press-img.svg", alt="", srcset="")
            .press-card2__text-wrap
                .press-card2__date April #{val} 2024
                .press-card2__text Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniamâ€¦
            a(href="http://google.com", target="_blank", rel="noopener noreferrer").press-card2__link
              span ReAD FULL ARTICLE
              <svg xmlns="http://www.w3.org/2000/svg" width="16.563" height="16.563" viewBox="0 0 16.563 16.563">
                <path id="Icon_ionic-md-arrow-back" data-name="Icon ionic-md-arrow-back" d="M11.712,5.124H2.818l4.1-4.1L5.856,0,0,5.856l5.856,5.856,1.025-1.025-4.062-4.1h8.894Z" transform="translate(16.563 8.282) rotate(135)" fill="#f4f3ec"/>
              </svg>
      .pagination
        a(href="#").pagination__arrow
          <svg xmlns="http://www.w3.org/2000/svg" width="11.321" height="11.321" viewBox="0 0 11.321 11.321">
            <path id="Icon_ionic-md-arrow-forward" data-name="Icon ionic-md-arrow-forward" d="M5.977,12.345h8.6l-3.962,3.962,1.026.991,5.66-5.66-5.66-5.66-.99.991,3.927,3.962h-8.6Z" transform="translate(17.297 17.297) rotate(180)" fill="#f4f3ec"/>
          </svg>
          span Prev
        .pagination__digits
          a(href="#").pagination__digit 1
          a(href="#").pagination__digit 2
          a(href="#").pagination__digit 3
          a(href="#").pagination__digit 4
          a(href="#").pagination__digit 5
        a(href="#").pagination__arrow
          span Next
          <svg xmlns="http://www.w3.org/2000/svg" width="11.321" height="11.321" viewBox="0 0 11.321 11.321">
            <path id="Icon_ionic-md-arrow-forward" data-name="Icon ionic-md-arrow-forward" d="M0,4.953H8.6L4.634.991,5.66,0l5.66,5.66-5.66,5.66-.99-.991L8.6,6.368H0Z" fill="#f4f3ec"/>
